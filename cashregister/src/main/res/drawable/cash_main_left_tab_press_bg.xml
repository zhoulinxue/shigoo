<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="0dp"
        android:bottom="0dp">
        <shape>
            <solid android:color="#292929" />
            <corners android:radius="4px" />
        </shape>
    </item>
    <item
        android:bottom="1.5dp"
        android:right="1.5dp"
        android:top="1.5dp"
        android:left="1.5dp">
        <shape>
            <gradient
                android:angle="0"
                android:startColor="#4FACFE"
                android:endColor="#1D95FF"/>
            <corners android:radius="4px" />
        </shape>
    </item>
</layer-list>


